{
	"name": "zptp_prpo_cycle_rep",
	"version": "0.0.1",
	"private": true,
	"sapux": true,
	"sapuxLayer": "CUSTOMER_BASE",
	"description": "",
	"keywords": [
		"ui5",
		"openui5",
		"sapui5"
	],
	"main": "webapp/index.html",
	"scripts": {
		"start": "fiori run --open \"test/flpSandbox.html?sap-client=030&sap-ui-xx-viewCache=false#zcds_prpo_cycle_hist-display\"",
		"start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandboxMockServer.html?sap-client=030&sap-ui-xx-viewCache=false#masterDetail-display\"",
		"start-noflp": "fiori run --open \"index.html?sap-client=030&sap-ui-xx-viewCache=false\"",
		"start-variants-management": "fiori run --open \"preview.html?sap-client=030&sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
		"build": "ui5 build -a --clean-dest --include-task=generateCachebusterInfo",
		"deploy": "fiori verify",
		"deploy-config": "fiori add deploy-config",
		"start-mock": "fiori run --open \"test/flpSandboxMockServer.html?sap-client=030&sap-ui-xx-viewCache=false#masterDetail-display\""
	},
	"devDependencies": {
        "@ui5/cli": "^3.0.0",
        "@sap/ux-ui5-tooling": "1",
        "rimraf": "3.0.2",
        "@sap/ux-specification": "UI5-1.52"
}
}
